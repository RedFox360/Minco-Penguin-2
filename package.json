{
  "name": "bs-poker-bot",
  "version": "1.0.0",
  "description": "Discord BS poker bot",
  "main": "main.ts",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/RedFox360/BS-Poker-Bot",
  "scripts": {
    "dev": "tsx ./src/main.ts",
    "dev-update": "tsx ./src/main.ts --update",
    "dev-main": "tsx ./src/main.ts --prod",
    "dev-main-update": "tsx ./src/main.ts --prod --update",
    "compile": "tsc",
    "start": "node ./compiled/main.js --prod",
    "start-update": "node ./compiled/main.js --prod --update",
    "lint": "eslint ./src",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.11",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "pretty-ms": "^9.0.0",
    "prisma": "^5.15.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "eslint": "^9.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "tsx": "^4.10.5",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "prisma": {
    "schema": "schema.prisma"
  }
}